<tr>
  <td>
    <%= code_climate[:name] %>
  </td>
  <td>
    <span class="h6 text-success"><%= code_climate[:code_climate_rate] %></span>
  </td>
  <td>
    <span class="code-climate-summary text-danger"><%= code_climate[:invalid_issues_count] %> invalid issues</span>
  </td>
  <td>
    <span class="code-climate-summary text-danger"><%= code_climate[:wont_fix_issues_count] %> won't fix issues</span>
  </td>
  <td>
    <span class="code-climate-summary text-danger"><%= code_climate[:open_issues_count] %> open issues</span>
  </td>
</tr>
